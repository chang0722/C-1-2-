#include <stdio.h>

int main() {
    int n; 
    printf("상품의 종류를 입력하시오:\n");
    scanf("%d", &n);

    int in[101], out[101], stock[101];

    printf("상품 별 입고수량 입력:\n");
    for (int i = 1; i <= n; i++) {
        scanf("%d", &in[i]);
    }

    printf("상품 별 판매수량 입력:\n");
    for (int i = 1; i <= n; i++) {
        scanf("%d", &out[i]);
    }

    
    for (int i = 1; i <= n; i++) {
        stock[i] = in[i] - out[i];
    }

    int id;
    printf("ID를 입력하시오:");
    scanf("%d", &id);

    
    printf("%d\n", stock[id]);

    
    for (int i = 1; i <= n; i++) {
        printf("%d ", stock[i]);
    }

    return 0;
}
